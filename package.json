{
  "name": "emoine_r-ui",
  "version": "0.0.0",
  "scripts": {
    "dev": "concurrently \"vite\" \"npm run start-mock\"",
    "build": "vite build",
    "lint": "eslint --ext .ts,.vue src",
    "type-check": "tsc",
    "gen-api": "node ./scripts/gen-api.js && buf generate https://github.com/traPtitech/Emoine_R.git#branch=feat/protobuf,subdir=api/proto",
    "start-mock": "prism mock -p 8090 -d https://raw.githubusercontent.com/traPtitech/Emoine_R/main/docs/openapi.yml"
  },
  "dependencies": {
    "@bufbuild/connect": "^0.11.0",
    "@bufbuild/connect-web": "^0.11.0",
    "@bufbuild/protobuf": "^1.3.0",
    "@iconify/vue": "^4.1.1",
    "@stoplight/prism-cli": "^5.0.1",
    "axios": "^1.4.0",
    "dayjs": "^1.11.7",
    "pinia": "^2.1.4",
    "reconnecting-websocket": "^4.4.0",
    "ress": "^5.0.2",
    "throttle-debounce": "^5.0.0",
    "vue": "^3.2.47",
    "vue-router": "^4.2.2"
  },
  "devDependencies": {
    "@bufbuild/buf": "^1.25.0",
    "@bufbuild/protoc-gen-connect-es": "^0.11.0",
    "@bufbuild/protoc-gen-es": "^1.3.0",
    "@openapitools/openapi-generator-cli": "^2.6.0",
    "@types/node": "^20.3.1",
    "@types/throttle-debounce": "^5.0.0",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.59.7",
    "@vitejs/plugin-vue": "^3.2.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.15.1",
    "prettier": "^2.8.8",
    "protobufjs": "^7.2.3",
    "sass": "^1.62.1",
    "ts-morph": "^18.0.0",
    "typescript": "^5.1.3",
    "vite": "^3.2.5"
  }
}
